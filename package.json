{
  "name": "kind-regards-earth-wp-theme",
  "version": "1.0.0",
  "private": true,
  "author": "Mike Vercoelen <mike.vercoelen@gmail.com",
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "precommit": "lint-staged",
    "clean": "rimraf ./build",
    "zip": "npm run build && node ./tools/zipTheme.js",
    "start": "npm run clean && NODE_ENV=development webpack --watch --config ./tools/config/webpackConfig.js",
    "build": "npm run clean && NODE_ENV=production webpack --config ./tools/config/webpackConfig.js",
    "dev": "npm run start",
    "prepush": "npm run build",
    "quick-deploy": "npm run build && git add . && git commit -a -m \"New build\" --no-verify && git push --no-verify"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-react"
    ],
    "env": {
      "browser": true
    },
    "globals": {
      "WPData": true,
      "WPSettings": true,
      "SiteSettings": true,
      "jQuery": true,
      "wp": true
    }
  },
  "devDependencies": {
    "archiver": "^2.1.1",
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "postcss-loader": "^2.1.5",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "blanket-animation": "^1.2.1",
    "classnames": "^2.2.6",
    "disqus-react": "^1.0.5",
    "dropcap.js": "^1.0.1",
    "he": "^1.1.1",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "include-media": "^1.4.9",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "react": "^16.4.1",
    "react-body-classname": "^1.2.0",
    "react-document-meta": "^3.0.0-beta.2",
    "react-dom": "^16.4.1",
    "react-headroom": "^2.2.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-transition-group": "^2.4.0",
    "react-youtube": "^7.6.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "sanitize.css": "^6.0.0",
    "smooth-scroll": "^14.2.0",
    "striptags": "^3.1.1",
    "webfontloader": "^1.6.28",
    "whatwg-fetch": "^2.0.4",
    "wpapi": "^1.1.2"
  }
}
